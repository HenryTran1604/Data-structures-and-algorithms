#include <bits/stdc++.h>
#define ll  long long
using namespace std;

int main() {
  ios::sync_with_stdio(0);
  cin.tie(0); cout.tie(0);
	int t; cin >> t;
	while(t--)
	{
		string s; cin >> s;
		for(int i = s.length()-1; i >= 0; i--)
		{
			if(s[i] == '1')
			{
				s[i] = '0';
			}
			else
			{
				s[i] = '1';
				break;
			}
		}
		cout << s << endl;
	}
	return 0;
}
